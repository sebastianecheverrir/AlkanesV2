journal = {Journal of Physics: Condensed Matter},
keywords = {elastohydrodynamic lubrication,lubricant flow,novel in situ measurement,shear banding},
month = {apr},
number = {13},
pages = {134005},
publisher = {IOP Publishing},
title = {{Flow measurements of a polyphenyl ether oil in an elastohydrodynamic contact}},
url = {http://dx.doi.org/10.1088/0953-8984/28/13/134005 http://stacks.iop.org/0953-8984/28/i=13/a=134005?key=crossref.0aa676b07832b50c3f5f4bd0e91dc5ad},
