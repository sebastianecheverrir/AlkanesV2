isbn = {1-56555-212-1},
keywords = {Aide d{\'{e}}cision,Ayuda decisi{\'{o}}n,Cojinete rodillos,Computer aided design,Concepci{\'{o}}n asistida,Conception assist{\'{e}}e,Decision aid,Decision support system,Desarrollo producto,D{\'{e}}veloppement produit,Expert system,Object oriented,Orientado objeto,Orient{\'{e}} objet,Palier roulement,Product development,Rolling bearing,Sistema ayuda decis{\'{i}}on,Sistema experto,Syst{\`{e}}me aide d{\'{e}}cision,Syst{\`{e}}me expert},
title = {{Roulexpert: un syst{\`{e}}me d'aide {\`{a}} la d{\'{e}}cision pour le choix des roulements}},
