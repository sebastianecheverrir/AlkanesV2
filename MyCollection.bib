pmid = {25607715},
title = {{The structures of hexadecylamine films adsorbed on iron-oxide surfaces in dodecane and hexadecane}},
url = {http://xlink.rsc.org/?DOI=C4CP05837B},
volume = {17},
year = {2015}
}
