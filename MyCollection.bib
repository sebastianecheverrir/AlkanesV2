@article{Finnis1996,
abstract = {The theory of metal–ceramic interfaces is a collection of approaches which are complementary. They range from thermodynamic modelling based on empirical correlations, through the image model of adhesion, semi-empirical tight-binding calculations, to first- principles calculations based on applying the density functional theory or Hartree–Fock theory. This article reviews the present state of theoretical calculations, with particular reference to electronic structure and adhesion. A section on the thermodynamic background clarifies the concept of work of adhesion which is the goal of many calculations. Cluster models and periodic slabs have been considered, both self-consistent and non-self-consistent. The most sophisticated and complete calculations have been made for metals on MgO and alumina. There a consistent picture of the nature of the bonding has emerged, although there are still significant unexplained discrepancies in numerical values.},
author = {Finnis, M W},
doi = {10.1088/0953-8984/8/32/003},
file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Finnis - 1996 - The theory of metal - ceramic interfaces.pdf:pdf},
isbn = {0953-8984},
issn = {0953-8984},
journal = {Journal of Physics: Condensed Matter},
month = {aug},
number = {32},
pages = {5811--5836},
title = {{The theory of metal - ceramic interfaces}},
url = {http://stacks.iop.org/0953-8984/8/i=32/a=003?key=crossref.6b2d897a5a15cc01a9a7a0826bfa6dab},
volume = {8},
year = {1996}
}
@article{Lozovoi2006a,
abstract = {First principles calculations of the Sigma 5 (310)[001] symmetric tilt grain boundary in Cu with Bi, Na, and Ag substitutional impurities provide evidence that in the phenomenon of Bi embrittlement of Cu grain boundaries electronic effects do not play a major role; on the contrary, the embrittlement is mostly a structural or "size" effect. Na is predicted to be nearly as good an embrittler as Bi, whereas Ag does not embrittle the boundary in agreement with experiment. While we reject the prevailing view that "electronic" effects (i.e., charge transfer) are responsible for embrittlement, we do not exclude the role of chemistry. However numerical results show a striking equivalence between the alkali metal Na and the semi metal Bi, small differences being accounted for by their contrasting "size" and "softness" (defined here). In order to separate structural and chemical effects unambiguously if not uniquely, we model the embrittlement process by taking the system of grain boundary and free surfaces through a sequence of precisely defined gedanken processes; each of these representing a putative mechanism. We thereby identify three mechanisms of embrittlement by substitutional impurities, two of which survive in the case of embrittlement or cohesion enhancement by interstitials. Two of the three are purely structural and the third contains both structural and chemical elements that by their very nature cannot be further unravelled. We are able to take the systems we study through each of these stages by explicit computer simulations and assess the contribution of each to the nett reduction in intergranular cohesion. The conclusion we reach is that embrittlement by both Bi and Na is almost exclusively structural in origin; that is, the embrittlement is a size effect.},
archivePrefix = {arXiv},
arxivId = {cond-mat/0608508},
author = {Lozovoi, A. Y. and Paxton, A. T. and Finnis, M. W.},
doi = {10.1103/PhysRevB.74.155416},
eprint = {0608508},
file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lozovoi, Paxton, Finnis - 2006 - Structural and chemical embrittlement of grain boundaries by impurities A general theory and first-p(2).pdf:pdf},
isbn = {1098-0121},
issn = {1098-0121},
journal = {Physical Review B},
month = {oct},
number = {15},
pages = {155416},
primaryClass = {cond-mat},
title = {{Structural and chemical embrittlement of grain boundaries by impurities: A general theory and first-principles calculations for copper}},
url = {http://link.aps.org/doi/10.1103/PhysRevB.74.155416},
volume = {74},
year = {2006}
}
