publisher = {Elsevier B.V.},
title = {{Drag-reducing paints for the reduction of fuel consumption in aviation and shipping}},
url = {http://dx.doi.org/10.1016/j.porgcoat.2010.09.026 http://linkinghub.elsevier.com/retrieve/pii/S0300944010003206},
