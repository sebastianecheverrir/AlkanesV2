issn = {13596454},
journal = {Acta Materialia},
keywords = {Fatigue modelling,Lenticular carbides,Martensite,Microstructural alteration,Rolling contact fatigue},
month = {apr},
pages = {176--187},
title = {{Modelling and characterisation of stress-induced carbide precipitation in bearing steels under rolling contact fatigue}},
