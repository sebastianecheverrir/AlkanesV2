keywords = {Additive interactions,MoDTC,Tribochemistry,Tribofilm,ZDDP},
number = {12},
pages = {1545--1557},
title = {{ZDDP and MoDTC interactions in boundary lubrication-The effect of temperature and ZDDP/MoDTC ratio}},
volume = {39},
year = {2006}
}
