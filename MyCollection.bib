file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Maras et al. - 2016 - Global transition path search for dislocation formation in Ge on Si(001).pdf:pdf},
issn = {00104655},
journal = {Computer Physics Communications},
keywords = {Diamond structure identification,Dislocation nucleation,Germanium on silicon,Global optimization of transition path},
month = {aug},
pages = {13--21},
publisher = {Elsevier B.V.},
title = {{Global transition path search for dislocation formation in Ge on Si(001)}},
url = {http://dx.doi.org/10.1016/j.cpc.2016.04.001 http://linkinghub.elsevier.com/retrieve/pii/S0010465516300893},
volume = {205},
year = {2016}
}
