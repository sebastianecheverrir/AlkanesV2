file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kumar et al. - 2008 - Tribochemistry in sliding wear of TiCN–Ni-based cermets.pdf:pdf},
isbn = {0884-2914},
