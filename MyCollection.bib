@misc{Hirth1973,
abstract = {Models for line defects in grain boundaries are considered. The structures of grain boundary dislocations (GBD's) and ledges are examined and line defects which correspond to either pure GBD's, pure grain boundary ledges or else defects possessing both dislocation and ledge character are distinguished. The concept of intrinsic grain boundary dislocations (IGBD's) is described, and circuits are denned which yield the Burgers vectors of both GBD's and IGBD's. Two distinct types of circuits are found to be useful and are presented. Virtual grain boundary dislocations (VGBD's) are also considered. What have been called VGBD's are shown in some cases to be true GBD's. In other cases, defects exist which could logically be termed VGBD's, but these can often alternatively be regarded as the result of the superposition of GBD's and IGBD's. GBD's formed in grain boundary cutting or impingement processes are found to be identical to secondary GBD's in boundaries of near-coincidence orientation. Generalized secondary GBD's are found to constitute an intermediate representation between the Frank model and the surface dislocation model.},
author = {Hirth, JP and Balluffi, RW},
booktitle = {Acta Metallurgica},
doi = {10.1016/0001-6160(73)90150-8},
file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Hirth, Balluffi - 1973 - On grain boundary dislocations and ledges.pdf:pdf},
