doi = {10.1103/PhysRevB.66.085420},
file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Qi et al. - 2002 - Friction anisotropy at Ni(100)(100) interfaces Molecular dynamics studies.pdf:pdf},
