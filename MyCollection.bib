@article{Gee1990,
abstract = {The shear forces between 2 molecularly smooth solid surfaces operated by thin films of various org. liqs. were measured to det. the nature of the transitions from continuum to mol. behavior in very thin films. For films whose thickness is {\textgreater}10 mol. diams., both their static and dynamic behavior usually can be described in terms of their bulk properties, but for thinner films their behavior becomes progressively more solidlike and can no longer be described, even qual., in terms of bulk/continuum properties such as viscosity. The solidlike state is characterized by the ordering of the liq. mols. into discrete layers. The mol. ordering is further modified by shear, which imposes a preferred orientation. All solidlike films exhibit a yield point or crit. shear stress, beyond which they behave like liq. crystals or ductile solids undergoing plastic deformation. Results for 5 liqs. of different mol. geometry reveal some very complex thin-film properties, such as the quantization of various static and dynamic properties, discontinuous or continuous solid-liq. transitions, smooth or stick-slip friction, and 2-dimensional nucleation. Quant., the "effective" viscosity in molecularly thin films can be 105 times the bulk value, and mol. relaxation times can be 1010 times slower. These properties depend not only on the nature of the liq., but also on the at. structure of the surfaces, the normal pressure, and the direction and velocity of sliding. Many thin-film properties depend on there being 2 surfaces close together and that they cannot be understood from a consideration of a single solid-liq. interface. The results provide new fundamental insights into the states of thin films, and have a bearing on understanding boundary friction, thin-film lubrication and the stress-strain properties of solids at the mol. level.},
author = {Gee, Michelle L. and McGuiggan, Patricia M. and Israelachvili, Jacob N. and Homola, Andrew M.},
doi = {10.1063/1.459067},
file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gee et al. - 1990 - Liquid to solidlike transitions of molecularly thin films under shear.pdf:pdf},
isbn = {0021-9606},
issn = {0021-9606},
journal = {The Journal of Chemical Physics},
month = {aug},
number = {3},
pages = {1895--1906},
title = {{Liquid to solidlike transitions of molecularly thin films under shear}},
url = {http://aip.scitation.org/doi/10.1063/1.459067},
volume = {93},
year = {1990}
}
