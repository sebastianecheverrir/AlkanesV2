@misc{Sutton1987,
abstract = {A review of geometric criteria for low interfacial energy which have been proposed in the literature is given. These include (i) low reciprocal volume density of coincidence sites; (ii) high planar density of coincidence sites, GAMMA ; (iii) high GAMMA at constant interplanar spacing, d; (iv) large d; and (v) high density of locked-in rows of atoms. These criteria are tested against experimental results which include measurements of: (a) interfacial energy; (b) rotations of crystallites on flat crystal substrates; (c) boundary faceting; (d) boundary dissociation; and (e) observations of grain boundary dislocations. No support for the general usefulness of criteria (i), (ii), (iv) and (v) is found. In all cases, results violating these criteria are found or else it is demonstrated that their range of validity is undefined, hence their predictive power is highly limited. Criterion (iii) applies for a limited number of cases involving metal/metal or ionic/ionic interfaces but fails for metal/ionic interfaces. It is concluded that no general and useful criterion for low energy can be enshrined in a simple geometric framework. Any understanding of the variations of interfacial energy must take account of the atomic structure and bonding at the interface.},
author = {Sutton, A.P and Balluffi, R.W},
booktitle = {Acta Metallurgica},
doi = {10.1016/0001-6160(87)90067-8},
file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Geometric, For, Interfacial - 1987 - ON GEOMETRIC CRITERIA FOR LOW INTERFACIAL ENERGY.pdf:pdf},
isbn = {0001-6160},
issn = {00016160},
month = {sep},
number = {9},
pages = {2177--2201},
title = {{Overview no. 61 On geometric criteria for low interfacial energy}},
url = {http://www.sciencedirect.com/science/article/pii/0001616087900678},
volume = {35},
year = {1987}
}
