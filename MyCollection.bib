pages = {034009},
title = {{Development of a quantum chemical molecular dynamics tribochemical simulator and its application to tribochemical reaction dynamics of lubricant additives}},
url = {http://stacks.iop.org/0965-0393/18/i=3/a=034009?key=crossref.5f726c2d957f63afa1d7565f1f175212},
