keywords = {high resolution images,research,risks management,sustainable reconstruction},
number = {3},
pages = {555},
title = {{Microstructural Alterations of Rolling—Bearing Steel Undergoing Cyclic Stressing}},
url = {http://fluidsengineering.asmedigitalcollection.asme.org/article.aspx?articleid=1432755},
volume = {88},
year = {1966}
}
