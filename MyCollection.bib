isbn = {0743-7463},
issn = {07437463},
journal = {Langmuir},
number = {16},
pages = {9533--9538},
pmid = {20225880},
title = {{A many-body dissipative particle dynamics study of spontaneous capillary imbibition and drainage}},
volume = {26},
year = {2010}
}
