@article{Wolf2005,
abstract = {We review the results of recent molecular-dynamics simulations of the structure and deformation behavior of nanocrystalline materials, i.e., polycrystalline materials with a grain size of typically less than about 100 nm. These simulations have now become large enough and sophisticated enough that they are beginning to cover the entire range of grain sizes over which the experimentally suggested transition from a dislocation-based deformation mechanism to one involving GB processes takes place. Their atomic-level resolution provides novel insights into the intricate interplay between the dislocation and GB processes responsible for this crossover. These simulations also reveal how and why this crossover in the dominant mechanism leads to a transition in the mechanical behavior. However, in spite of these early successes, these simulations are inherently limited to rather idealized model microstructures and extremely high deformation rates. We therefore address the critical question as to the degree to which they begin to capture the experimentally observed, albeit controversial, deformation behavior of real nanocrystalline materials. (Supplementary material to this article, in the form of color graphs of some of the figures and several deformation-simulation movies, can be viewed at http://phillpot.mse.ufl.edu/review.html.) {\textcopyright} 2004 Acta Materialia Inc. Published by Elsevier Ltd. All rights reserved.},
author = {Wolf, D. and Yamakov, V. and Phillpot, S. R. and Mukherjee, A. and Gleiter, H.},
doi = {10.1016/j.actamat.2004.08.045},
file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wolf et al. - 2005 - Deformation of nanocrystalline materials by molecular-dynamics simulation relationship to experiments.pdf:pdf},
isbn = {1359-6454},
issn = {13596454},
journal = {Acta Materialia},
month = {jan},
number = {1},
pages = {1--40},
title = {{Deformation of nanocrystalline materials by molecular-dynamics simulation: Relationship to experiments?}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1359645404005087},
volume = {53},
year = {2005}
}
@article{Sheppard2008,
abstract = {A comparison of chain-of-states based methods for finding minimum energy pathways (MEPs) is presented. In each method, a set of images along an initial pathway between two local minima is relaxed to find a MEP. We compare the nudged elastic band (NEB), doubly nudged elastic band, string, and simplified string methods, each with a set of commonly used optimizers. Our results show that the NEB and string methods are essentially equivalent and the most efficient methods for finding MEPs when coupled with a suitable optimizer. The most efficient optimizer was found to be a form of the limited-memory Broyden-Fletcher-Goldfarb-Shanno method in which the approximate inverse Hessian is constructed globally for all images along the path. The use of a climbing-image allows for finding the saddle point while representing the MEP with as few images as possible. If a highly accurate MEP is desired, it is found to be more efficient to descend from the saddle to the minima than to use a chain-of-states method with many images. Our results are based on a pairwise Morse potential to model rearrangements of a heptamer island on Pt(111), and plane-wave based density functional theory to model a rollover diffusion mechanism of a Pd tetramer on MgO(100) and dissociative adsorption and diffusion of oxygen on Au(111).},
author = {Sheppard, Daniel and Terrell, Rye and Henkelman, Graeme},
doi = {10.1063/1.2841941},
file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sheppard, Terrell, Henkelman - 2008 - Optimization methods for finding minimum energy paths.pdf:pdf},
issn = {0021-9606},
journal = {The Journal of Chemical Physics},
month = {apr},
number = {13},
pages = {134106},
pmid = {18397052},
title = {{Optimization methods for finding minimum energy paths.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18397052},
volume = {128},
year = {2008}
}
