@article{Ewen2016,
abstract = {For the successful development and application of lubricants, a full understanding of the nanoscale behavior of complex tribological systems is required, but this is difficult to obtain experimentally. In this study, we use nonequilibrium molecular dynamics (NEMD) simulations to examine the atomistic structure and friction properties of commercially relevant organic friction modifier (OFM) monolayers adsorbed on iron oxide surfaces and lubricated by a thin, separating layer of hexadecane. Specifically, acid, amide, and glyceride OFMs, with saturated and Z-unsaturated hydrocarbon tail groups, are simulated at various surface coverages and sliding velocities. At low and medium coverage, the OFMs form liquidlike and amorphous monolayers, respectively, which are significantly interdigitated with the hexadecane lubricant, resulting in relatively high friction coefficients. At high coverage, solidlike monolayers are formed for all of the OFMs, which, during sliding, results in slip planes between well-defined OFM and hexadecane layers, yielding a marked reduction in the friction coefficient. When present at equal surface coverage, OFMs with saturated and Z-unsaturated tail groups are found to yield similar structure and friction behavior. OFMs with glyceride head groups yield significantly lower friction coefficients than amide and particularly carboxylic acid head groups. For all of the OFMs and coverages simulated, the friction coefficient is found to increase linearly with the logarithm of sliding velocity; however, the gradient of this increase depends on the coverage. The structure and friction details obtained from these simulations agree well with experimental results and also shed light on the relative tribological performance of these OFMs through nanoscale structural variations. This has important implications in terms of the applicability of NEMD to aid the development of new formulations to control friction.},
author = {Ewen, James P. and Gattinoni, Chiara and Morgan, Neal and Spikes, Hugh A. and Dini, Daniele},
doi = {10.1021/acs.langmuir.6b00586},
file = {:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ewen et al. - 2016 - Nonequilibrium Molecular Dynamics Simulations of Organic Friction Modifiers Adsorbed on Iron Oxide Surfaces.pdf:pdf;:home/sebastian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ewen et al. - 2016 - Nonequilibrium Molecular Dynamics Simulations of Organic Friction Modifiers Adsorbed on Iron Oxide Surfaces(2).pdf:pdf},
isbn = {0743-7463},
issn = {15205827},
journal = {Langmuir},
month = {may},
number = {18},
pages = {4450--4463},
pmid = {27064962},
title = {{Nonequilibrium molecular dynamics simulations of organic friction modifiers adsorbed on iron oxide surfaces}},
url = {http://pubs.acs.org/doi/abs/10.1021/acs.langmuir.6b00586},
volume = {32},
year = {2016}
}
