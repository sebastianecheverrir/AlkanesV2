url = {https://doi.org/10.1016/j.fuel.2017.11.142 http://linkinghub.elsevier.com/retrieve/pii/S0016236117315478},
volume = {220},
year = {2018}
}
@article{Gao2000,
abstract = {Investigations of surface roughness effects on the structure, dynamics$\backslash$nand rheology of a molecular fluid (hexadecane) confined between solid$\backslash$n(gold) surfaces, through the use of large-scale molecular dynamics$\backslash$nsimulations, reveal a remarkable sensitivity to the confining surface$\backslash$nmorphology. A most significant reduction of the ordering propensity$\backslash$nis found in films confined by stationary rough surfaces with a consequent$\backslash$nstrong suppression of solvation forces and the development of liquid-like$\backslash$ndynamic and response characteristics. When the rough-surface boundaries$\backslash$nare set in motion at a high shear rate, the interfacial layers of$\backslash$nthe film stick to the adjacent solid boundaries, resulting in partial$\backslash$nslip inside the film with the development of shear stress in the$\backslash$nviscous molecular fluid, unlike the case of atomically flat crystalline$\backslash$nboundaries where slip of the confined film at the boundaries is accompanied$\backslash$nby vanishingly small shear stress in the film. These results are$\backslash$ndiscussed in the context of the effect of roughness on the boundary$\backslash$nconditions used in modeling fluid flow past surfaces, and they suggest$\backslash$nthat morphological patterning of surfaces could provide ways for$\backslash$ncontrolled modifications of frictional processes in thin-film lubricated$\backslash$nnanotribological systems.},
